local oldNamecall = mt.__namecall
mt.__namecall = newcclosure(function(self, ...)
    if getnamecallmethod() == "Kick" and self == LocalPlayer then
        return warn("Kick attempt blocked by BurakSilentSpawner")
    end
    return oldNamecall(self, ...)
end)

-- Pet & Egg Functions
