    game:GetService("RunService").Heartbeat:Connect(function()
        if not flying then bv:Destroy() bg:Destroy() return end
        local cam = workspace.CurrentCamera
        bv.Velocity = cam.CFrame.LookVector * 80
        bg.CFrame = cam.CFrame
    end)
end

